{
    "id": "rmu-character-sheet-exporter",
    "title": "RMU Character Sheet Exporter",
    "description": "A module to export character sheets using different templates and styles.",
    "version": "1.4.0",
    "compatibility": {
        "minimum": "13",
        "verified": "13.351"
    },
    "authors": [{ "name": "Ken Dearman" }],
    "esmodules": ["main.js"],
    "languages": [
        { "lang": "en", "name": "English", "path": "languages/en.json" },
        {
            "lang": "en-GB",
            "name": "English (UK)",
            "path": "languages/en-gb.json"
        },
        {
            "lang": "es",
            "name": "Español (Spanish)",
            "path": "languages/es.json"
        },
        {
            "lang": "es-419",
            "name": "Español (Spanish - Latin America)",
            "path": "languages/es-419.json"
        },
        {
            "lang": "fr",
            "name": "Français (French)",
            "path": "languages/fr.json"
        },
        {
            "lang": "sv",
            "name": "Svenska (Swedish)",
            "path": "languages/sv.json"
        }
    ],
    "styles": ["styles/filroden-theme.css", "styles/rmu-cse.css"],
    "url": "https://github.com/Filroden/rmu-character-sheet-exporter",
    "manifest": "https://github.com/Filroden/rmu-character-sheet-exporter/releases/latest/download/module.json",
    "download": " https://github.com/Filroden/rmu-character-sheet-exporter/releases/download/1.4.0/rmu-character-sheet-exporter.zip",
    "bugs": "https://github.com/Filroden/rmu-character-sheet-exporter/issues",
    "readme": "https://github.com/Filroden/rmu-character-sheet-exporter/blob/main/README.md",
    "license": "https://mit-license.org/"
}
